<!DOCTYPE html>
<html lang="fa" dir="rtl">
<head>
  <%- include("./templates/header.ejs", {title : "آیفون استور", css : "homePage"}) %>
</head>
<body>
  <%- include("./templates/sideBar.ejs", {active : "logs"}) %>
  <div id="check-container">
    <div>
      <h1>آخرین سوابق</h1>
    </div>
    <% if (logs[0]) { %>
        <% logs.forEach(log => { %>
            <%- include("./templates/log.ejs", {log}) %>
        <% }) %>
    <% }else{ %>
      <div class="err">سابقه‌ای یافت نشد.</div>
    <% } %>
  </div>
</body>
</html>